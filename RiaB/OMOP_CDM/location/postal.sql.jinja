SELECT
  CONCAT("POSTAL_", postal_code) AS location_id,
  CAST(NULL AS STRING) AS address_1,
  CAST(NULL AS STRING) AS address_2,
  city AS city,
  CAST(NULL AS STRING) AS state,
  postal_code AS zip,
  country AS county,
  postal_code AS location_source_value,
  CONCAT('POSTAL_', country_code) AS country_concept_id,
  country_code AS country_source_value,
  CAST(NULL AS FLOAT64) AS latitude,
  CAST(NULL AS FLOAT64) AS longitude
FROM
  {{project_raw}}.raw_data.postal
